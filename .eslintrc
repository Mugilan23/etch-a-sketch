{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "camelcase": "off",
        "no-plusplus": "off",
        "prefer-const": "off",
        "no-shadow": "off",
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4,
            }
        ]
    }
}